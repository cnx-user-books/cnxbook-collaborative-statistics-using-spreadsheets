  <m:mo>=</m:mo>
  <m:mfrac>
    <m:mrow>
      <m:mrow>
        <m:mo>(</m:mo>
        <m:msub>
          <m:mrow>
      <m:mover><m:mi>p</m:mi><m:mo>̂</m:mo></m:mover>
