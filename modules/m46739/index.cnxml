      <m:mn>200</m:mn>
    </m:mrow>
  </m:mfrac>
  <m:mo>=</m:mo>
  <m:mn>0.08</m:mn><m:mspace width="12pt"/> <m:mn>1</m:mn>
  <m:mo>−</m:mo>
  <m:msub>
<m:mover><m:mi>p</m:mi><m:mo>̂</m:mo></m:mover>
<m:mi>pooled</m:mi>
</m:msub>
  <m:mo>=</m:mo>
  <m:mn>0.92</m:mn> 	
</m:math></para><para id="element-3">Therefore, 
<m:math>
<m:mspace width="10pt"/>
 <m:msub>
    <m:mrow>
