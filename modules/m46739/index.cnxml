        <m:mi>B</m:mi>
      </m:msub>
    </m:mrow>
  </m:mfrac>
  <m:mo>=</m:mo>
  <m:mfrac>
    <m:mrow>
      <m:mn>20</m:mn>
      <m:mo>+</m:mo>
      <m:mn>12</m:mn>
    </m:mrow>
    <m:mrow>
      <m:mn>200</m:mn>
